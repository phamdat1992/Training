\par }{\f15\fs24\ul C\'b8ch tr\'d7nh b\'b5y 1 :
\par }{\fs24 uses  crt;
\par const max    = 101;
\par       fi     = 'luong.inp';
\par       fo     = 'luong.tdh';
\par 
\par type  mang   = array[0..max,0..max] of integer;
\par       mang2  = array[0..max] of integer;
\par       rec    = record
\par                     delta,toi:integer;
\par                end;
\par       mang3  = array[0..max] of rec;
\par 
\par var   a,F    : mang;
\par       nh     : mang3;
\par       s,dx   : mang2;
\par       n,top  : integer;
\par 
\par procedure docf;
\par var i,j:integer;
\par     f : text;
\par begin
\par      assign(f,fi);
\par      reset(f);
\par      readln(f,n);
\par      for i:=1 to n do
\par          begin
\par               for j:=1 to n do
\par                   read(f,a[i,j]);
\par               readln(f);
\par          end;
\par      close(f);
\par end;
\par 
\par procedure nap(p:integer);
\par begin
\par      inc(top);
\par      s[top]:=p;
\par end;
\par 
\par function lay:integer;
\par begin
\par      lay:=s[top];
\par      dec(top);
\par end;
\par 
\par function min(a,b:integer):integer;
\par begin
\par      if a<b then min:=a
\par      else min:=b;
\par end;
\par 
\par procedure init;
\par begin
\par      fillchar(dx,sizeof(dx),0);
\par      top:=1;
\par      s[top]:=1;
\par      nh[1].delta:=maxint;
\par      nh[1].toi:=1;
\par end;
\par 
\par procedure suanhan(j:integer);
\par var i:integer;
\par begin
\par      for i:=1 to n do
\par          if dx[i]=0 then
\par             begin
\par                  if (a[j,i]<>0) and (F[j,i]<a[j,i]) then
\par                     begin
\par                          dx[i]:=1;
\par                          nh[i].toi:=+j;
\par                          nh[i].delta:=
\par                             min(nh[j].delta,a[j,i]-F[j,i]);
\par                          nap(i);
\par                     end
\par                  else
\par                      if (a[i,j]<>0) and (F[i,j]>0) then
\par                         begin
\par                              dx[i]:=1;
\par                              nh[i].toi:=-j;
\par }\pard \s16\li3600\nowidctlpar\adjustright {\fs24     nh[i].delta:=
\par           min(nh[j].delta,F[i,j]);
\par }\pard \s16\nowidctlpar\adjustright {\fs24                              nap(i);
\par                         end
\par             end;
\par end;
\par 
\par procedure tangluong;
\par var z,x:integer;
\par begin
\par      z:=n;
\par      repeat
\par            x := z;
\par            z := nh[x].toi;
\par            if z>0 then inc(F[z,x],nh[n].delta)
\par            else        dec(F[x,z],nh[n].delta);
\par            z:=abs(z);
\par      until z=1;
\par end;
\par 
\par procedure hienkq;
\par var i,j,gt : integer;
\par     g      : text;
\par begin
\par      gt := 0;
\par      for i:=2 to n do
\par          if F[1,i]>0 then gt:=gt+F[1,i];
\par      assign(g,fo);
\par      rewrite(g);
\par      writeln(g,gt);
\par      for i:=1 to n do
\par          for j:=1 to n do
\par              if F[i,j]>0 then
\par              writeln(g,i,' ',j,' ',F[i,j]);
\par      close(g);
\par end;
\par 
\par procedure timluong;
\par var v:integer;
\par begin
\par      repeat
\par            init;
\par            while top>0 do
\par                  begin
\par                     v:=lay;
\par                     suanhan(v);
\par }\pard \s16\li2880\nowidctlpar\adjustright {\fs24 if dx[n]<>0 then 
\par begin
\par }\pard \s16\fi720\li2880\nowidctlpar\adjustright {\fs24 tangluong;
\par break;}{\i \{}{\i\f15 l\'d6nh ng\'bet n\'b5y l\'b5m ch/tr\'d7nh ch\'b9y t\'e8t h\'acn nhi\'d2u \}
