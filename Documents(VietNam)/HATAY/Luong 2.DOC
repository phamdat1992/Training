\par        u          : set of byte;
\par 
\par procedure docfile;
\par        var f : text;
\par            i,j : integer;
\par        begin
\par             assign(f,fi);
\par             reset(f);
\par             readln(f,n,a,z);
\par             for i:=1 to n do
\par             begin
\par                  for j:=1 to n do read(f,c[i,j]);
\par                  readln(f);
\par             end;
\par             close(f);
\par        end;
\par 
\par function chon : byte;
\par        var i : byte;
\par        begin
\par             chon := 0;
\par             for i:=1 to n do
\par                if i in u then
\par                begin
\par                    chon := i;
\par                    exit;
\par                end;
\par        end;
\par 
\par function min(a,b : integer) : integer;
\par        begin
\par             if a<b then min := a else min := b;
\par        end;
\par 
\par procedure max_flow;
\par        var i,j,d,w : integer;
\par begin
\par        for i:=1 to n do
\par        for j:=1 to n do f[i,j] := 0;
\par 
\par        while true do
\par        begin
\par }{\f15\fs22 \{ Kh\'ebi tr\'de \}}{\fs22     
\par        for i:= 1 to n do
\par                begin
\par                      pre[i] := 0;
\par                      val[i] := 0;
\par                end;
\par            pre[a] := a;
\par            val[a] := Maxint;
\par            u      := [a];
\par 
\par }{\f15\fs22 \{ S\'f6a nh\'b7n \}
\par }{\fs22            while val[z]=0 do
\par            begin
\par                 if u=[] then exit;
\par                 v := chon;
\par                 u := u-[v];
\par                 d := val[v];
\par                 for w:=1 to n do
\par                 if val[w]=0 then
\par                 if f[v,w]<c[v,w] then
\par                 begin
\par                      pre[w] := v;
\par                      val[w] := min(d,c[v,w]-f[v,w]);
\par                      u      := u+[w];
\par                 end;
\par                 for w:=1 to n do
\par                 if val[w]=0 then
\par                 if c[w,v]>0 then
\par                 if f[w,v]>0 then
\par                 begin
\par                      pre[w] := v;
\par                      val[w] := min(d,f[w,v]);
\par                      u      := u+[w];
\par                 end;
\par            end;
\par 
\par }{\f15\fs22 \{ T\'d7m d\'a9ychuy\'d2n \}
\par 
\par }{\fs22            w := z;
\par            k := 0;
\par            while w<>a do
\par            begin
\par                  kq[k] := w;
\par                  w     := pre[w];
\par                  inc(k);
\par            end;
\par            kq[k] := a;
\par 
\par            d := val[z];
\par            if d= 0 then exit;
\par }{\f15\fs22 
\par \{ T\'a8ng lu\'e5ng \}
\par }{\fs22            for i:= k downto 1 do
\par            begin
\par 
\par                 if c[kq[i],kq[i-1]]>0 then
\par                     f[kq[i],kq[i-1]] := f[kq[i],kq[i-1]] + d;
\par 
\par                 if c[kq[i-1],kq[i]]>0 then
\par                     f[kq[i-1],kq[i]] := f[kq[i-1],kq[i]] - d;
\par            end;
\par 
\par        end;
\par end;
\par 
\par procedure hien;
\par        var i,j,gt : integer;
\par            g      : text;
\par        begin
\par             assign(g,fo);
\par             rewrite(g);
\par             gt := 0;
\par             for i:= 2 to n do
\par                 gt := gt + f[1,i];
\par             writeln(g,gt);
\par             for i:= 1 to n do
\par             for j:=1 to n do
\par                if f[i,j] >0 then
\par                    writeln(g,i,' ',j,' ',f[i,j]);
\par             close(g);
\par        end;
\par 
\par BEGIN
\par        clrscr;
\par        docfile;
\par        max_flow;
\par        hien;
\par END.
