\par          p,q,r,ini : mg;
\par          tr,             }{\f15 \{Theo r\'e2i d\'a9y chuy\'d2n \}
\par }\pard \s16\li720\nowidctlpar\adjustright {   trai,phai : mg; }{\f15 \{T\'ab m\'b5u c\'b8c cung tren d\'a9y chuy\'d2n\}}{
\par }\pard \s16\nowidctlpar\adjustright {         so        : array[1..3] of word;
\par          n,m,k,
\par }\pard \s16\fi720\nowidctlpar\adjustright {   Lj  \tab  : word;
\par }\pard \s16\nowidctlpar\adjustright {         maxs,mins : longint; }{\f15 \{Max th\'eai gian, Min th\'eai gian\}}{
\par          giua      : longint; }{\f15 \{Th\'eai gian d\'f9 ki\'d5n cho m\'e9t ph\'ad\'acng \'b8n = gi\'b8 tr\'de d\'f9 ki\'d5n l\'b5 th\'abng l\'ad\'eeng   lu\'e5ng l\'edn nh\'cat tr\'aan m\'e4i cung\}}{ 
\par          ok        : boolean;
\par 
\par procedure init;
\par var  i    : word;
\par begin
\par      fillchar(ini,sizeof(ini),0);
\par      for i := 1 to n do
\par          begin
\par               new(a[i]);
\par               a[i]^ := ini;
\par          end;
\par end;
\par 
\par procedure docf;
\par var  i,j,k,x,y : word;
\par      f         : text;
\par begin
\par      assign(f,fi);
\par      reset(f);
\par      readln(f,n,m,k);
\par      init;
\par      maxs := 0;  mins := 65000;
\par      for i := 1 to n do readln(f,p[i],q[i]);
\par      for j := 1 to m do
\par          begin
\par               readln(f,r[j]);
\par               for k := 1 to r[j] do
\par                   begin
\par                        read(f,x);
\par                        for y := 1 to 3 do
\par                            begin
\par                                 read(f,so[y]);
\par                                 if so[y] > maxs then maxs := so[y];
\par                                 if so[y] < mins then mins := so[y];
\par                            end;
\par                        for i := 1 to n do
\par                            if p[i] = x then
\par                               a[i]^[j] := so[q[i]];
\par                        readln(f);
\par                   end;
\par          end;
\par      close(f);
\par end;
\par 
\par procedure ghep(i  :   word);
\par var  j : word;
\par begin
\par      if ok then exit;
\par      for j := 1 to m do
\par          if tr[j] = 0 then}{\f15 \{ t\'d7m  \'ae\'d8nh ch\'ada gh\'d0p v\'b5o d\'a9y chuy\'d2n (nh\'a9n vi\'aan j ch\'ada nh\'cbn c\'abng vi\'d6c n\'b5o)\}
\par }{            if (a[i]^[j] > 0) and (a[i]^[j] <= giua) then
\par \tab \tab }{\f15 \{ \'a7i\'d2u ki\'d6n cung (i,j) l\'b5 cung \'ae\'cbm : c\'abng vi\'d6c c\'f1a kh\'b8ch h\'b5ng i \'ae\'dfi h\'e1i nh\'a9n vi\'aan j l\'b5m v\'edi th\'eai gian kh\'abng qu\'b8 th\'eai gian chung t\'e8i \'aea c\'f1a m\'e4i ng\'ad\'ea
i (= gi\'f7a) \}
\par }{               begin
\par                     tr[j] := i; \{}{\f15 x\'b8c nh\'cbn : giao cho nh\'a9n vi\'aan j c\'abng vi\'d6c c\'f1a kh\'b8ch i\} }{
\par                     if trai[j] = 0 then }{\f15 \{ H\'d5t d\'a9y chuy\'d2n\} }{
\par                        begin
\par                             ok := true;
\par                             Lj := j;
\par                             exit;
\par                        end
\par                     else ghep(trai[j]);\{}{\f15 T\'d7m ti\'d5p d\'a9y chuy\'d2n b\'bbng \'ae\'d6 quy\} }{
\par                end;
\par end;
\par 
\par procedure tang; }{\f15 \{ T\'a8ng th\'aam 1 c\'c6p gh\'d0p (t\'a8ng th\'aam 1 nh\'a9n vi\'aan \'ae\'ad\'eec giao vi\'d6c) \}
\par }{var  luu  : word;
\par begin
\par      repeat
\par            trai[Lj]:= tr[Lj];}{\f15 \{\'ae\'b8nh d\'cau \'ae\'d8nh \'ae\'cbm b\'aan tr\'b8i (kh\'b8ch h\'b5ng tr[Lj] \'ae\'b7 \'ae\'ad\'eec x\'d5p)\} }{
\par            luu     := phai[tr[lj]];}{\f15 \{bi\'d5n ph\'f4 gi\'f7 l\'b9i gi\'b8 tr\'de \'ae\'d8nh b\'aan ph\'b6i ti\'d5p theo c\'f1a d\'a9y chuy\'d2n\}}{
\par            phai[tr[Lj]] := Lj;}{\f15  \{\'ae\'b8nh d\'cau \'ae\'d8nh \'ae\'cbm b\'aan ph\'b6i (nh\'a9n vi\'aan tr[Lj] \'ae\'b7 nh\'cbn vi\'d6c)\}}{
\par            Lj           := luu;}{\f15  \{x\'d0t \'ae\'d8nh b\'aan ph\'b6i ti\'d5p theo c\'f1a d\'a9y chuy\'d2n (nh\'a9n vi\'aan ti\'d5p theo) \}}{
\par      until Lj = 0; }{\f15 \{K\'d5t th\'f3c d\'a9y chuy\'d2n t\'b9i \'ae\'d8nh nh\'b9t b\'aan ph\'b6i\}}{
\par end;
\par 
\par procedure inic;
\par begin
\par     trai := ini;
\par     phai := ini;
\par end;
\par 
\par procedure capghep;
\par var  i    : word;
\par begin
\par      inic;
\par      for i := 1 to n do
\par          begin
\par               ok := false;
\par               tr := ini;
\par               ghep(i);
\par               if ok then tang
\par               else exit;
\par          end;
\par end;
\par 
\par procedure lamv;
\par var  dau,cuoi  : longint;
\par begin
\par      dau := mins;
\par      cuoi := maxs;
\par      repeat
\par            giua := (dau + cuoi) div 2;
\par            capghep;
\par            if ok then cuoi := giua
\par            else dau := giua + 1;
\par      until dau >= cuoi;
\par      giua := cuoi;
\par      capghep;
\par end;
\par 
\par procedure ghif;
\par var  f    : text;
\par      i    : word;
\par begin
\par      assign(f,fo);
\par      rewrite(f);
\par      if ok then
\par         begin
\par              writeln(f,giua);
\par              for i := 1 to n do writeln(f,phai[i]);
\par         end
\par      else write(f,-1);
\par      close(f);
\par end;
\par 
\par procedure dispos;
\par var  i : word;
\par begin
\par      for i:=1  to n do dispose(a[i]);
\par end;
\par 
\par BEGIN
\par      clrscr;
\par      docf;
\par      lamv;
\par      ghif;
\par      dispos;
\par END.
\par }{\fs24 uses crt;
\par const        max          = 200;
\par              fi           = 'somnhat.in5';
\par              fo           = 'dong.out';
\par type         mg           = array[1..max]of integer;
\par var          a            : array[1..max]of ^mg;
\par              trai,phai,lq : array[1..max]of integer;
\par              tr,dx,q      : array[1..2*max]of integer;
\par              lv,dk        : array[1..max]of integer;
\par              m,n,vd       : integer;
\par              dau,cuoi     : longint;
\par              best         : integer;
\par              ok           : boolean;
\par procedure inp;
\par var  f     : text;
\par      i,j,k : integer;
\par      so    : integer;
\par      gt    : array[1..3]of integer;
\par      vde   : integer;
\par  begin
\par    dau:=maxint;cuoi:=0;
\par    assign(f,fi);
\par    reset(f);
\par    readln(f,n,m,vd);
\par    for k:=1 to n do readln(f,lv[k],dk[k]);
\par    for i:=1 to m do
\par      begin
\par        readln(f,so);
\par        for j:=1 to so do
\par          begin
\par            readln(f,vde,gt[1],gt[2],gt[3]);
\par            for k:=1 to n do
\par             if lv[k]=vde then
\par               begin
\par                 a[k]^[i]:=gt[dk[k]];
\par                 if cuoi<a[k]^[i] then cuoi:=a[k]^[i];
\par                 if dau>a[k]^[i] then dau:=a[k]^[i];
\par               end;
\par          end;
\par      end;Z
\par    close(f);
\par  end;
\par procedure dieuchinh(j:integer);
\par var i     : integer;
\par  begin
\par    repeat
\par      i:=tr[j];
\par      trai[i]:=j-n;
\par      phai[j-n]:=i;
\par      j:=tr[i];
\par  end;
\par procedure work(tg:longint);
\par var k,i   : integer;
\par     dd,cc : integer;
\par     stop  : boolean;
\par  begin
\par    fillchar(trai,sizeof(trai),0);
\par    fillchar(phai,sizeof(phai),0);
\par    for k:=1 to n do
\par      begin
\par        fillchar(q,sizeof(q),0);
\par        fillchar(dx,sizeof(dx),0);
\par        fillchar(tr,sizeof(tr),0);
\par        dd:=1;cc:=1;dx[k]:=1;q[1]:=k;
\par        stop:=false;
\par        while (dd<=cc)and(stop=false) do
\par          begin
\par            if q[dd]<=n then
\par              begin
\par                for i:=1 to m do
\par                  if (dx[i+n]=0)and(a[q[dd]]^[i]<=tg) then
\par                    begin
\par                      inc(cc);q[cc]:=i+n;
\par }\pard \s16\fi720\li2160\nowidctlpar\adjustright {\fs24  dx[i+n]:=1;tr[q[cc]]:=q[dd];
\par }\pard \s16\nowidctlpar\adjustright {\fs24                      if phai[i]=0 then
\par                        begin
\par                          dieuchinh(i+n);
\par                          stop:=true;
\par                          break;
\par                        end;
\par                    end;
\par              end
\par            else if dx[phai[q[dd]-n]]=0 then
\par              begin
\par                inc(cc);
\par                q[cc]:=phai[q[dd]-n];
\par }\pard \s16\fi720\li720\nowidctlpar\adjustright {\fs24      dx[q[cc]]:=1;tr[q[cc]]:=q[dd];             end;           inc(dd);         end;       if stop=false then exit;
\par }\pard \s16\nowidctlpar\adjustright {\fs24      end;
\par    ok:=true;
\par  end;
\par procedure solution;
\par var tg : longint;
\par  begin
\par    best:=maxint;
\par    while dau<=cuoi do
\par      begin
\par         tg:=(dau+cuoi)div 2;
\par         ok:=false;
\par         work(tg);
\par         if ok then
\par           begin
\par             lq:=trai;
\par             best:=tg;
\par             cuoi:=tg-1;
\par           end
\par         else dau:=tg+1;
\par      end;
\par  end;
\par procedure out;
\par var  f    : text;
\par      k    : integer;
\par  begin
\par    assign(f,fo);
\par    rewrite(f);
\par    if best<maxint then
\par      begin
\par       writeln(f,best);
\par       for k:=1 to n do  writeln(f,lq[k]);
\par      end
\par    else write(f,'-1');
\par    close(f);
\par  end;
\par procedure news;
\par var k,kk     : integer;
\par  begin
\par    for k:=1 to max do
\par      begin
\par        new(a[k]);
\par        fillchar(a[k]^,sizeof(a[k]^),0);
\par        for kk:=1 to max do a[k]^[kk]:=maxint;
\par      end;
\par  end;
\par procedure disposes;
\par var k     : integer;
\par  begin
\par    for k:=1 to max do dispose(a[k]);
\par  end;
\par BEGIN
