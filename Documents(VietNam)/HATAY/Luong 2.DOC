\par }\pard \s16\nowidctlpar\adjustright {\b\f15\fs28\ul B\'b5i 3 :}{\f15\fs28    C\'b8ch 1 
\par }{\fs24 
\par uses    crt;
\par const   max \tab = 100;
\par         fi     = 'input.txt';
\par         fo     = 'qg_95b1.out';
\par type    kpt    = record
\par                         truoc : integer;
\par                         delta : byte;
\par                   end;
\par         knhan      = array[1..2*max+2] of kpt;
\par         kstack     = array[1..2*max+2] of byte;
\par         kdagannhan = array[1..2*max+2] of boolean;
\par 
\par         kmang      = array[1..2*max+2,1..2*max+2] of byte;
\par 
\par var     nh         : knhan;
\par         s          : kstack;
\par         c          : kmang;
\par         d          : kdagannhan;
\par         m,n,k,lk,i,top,xp,dich,maxtc,mintc,sc : byte;
\par         ok : boolean;
\par         time:longint absolute 0:$46c;
\par         ltime:longint;
\par 
\par procedure taof;
\par var i,j,k : byte;
\par     f:text;
\par begin
\par      assign(f,fi);
\par      rewrite(f);
\par      randomize;
\par      writeln(f,max,' ',max div 10);
\par      for i:=1 to max do
\par          begin
\par               write(f,1,1);
\par               for j:=3 to max do
\par               begin
\par                   k := random(5);
\par                   if k<>1 then k := 0;
\par                   write(f,k);
\par               end;
\par               writeln(f);
\par          end;
\par      close(f);
\par end;
\par 
\par procedure docf;
\par var i,j : byte; f : text; ch : char;
\par     tc : array[1..max] of byte;
\par begin
\par      fillchar(tc,sizeof(tc),0);
\par      fillchar(c,sizeof(c),0);
\par      assign(f,fi);
\par      reset(f);
\par      if ioresult<>0 then
\par         begin
\par \tab      writeln('khong tim thay file ',fi);
\par \tab      readln;
\par \tab      halt;
\par \tab end;
\par      readln(f,m,n);
\par      xp   \tab := 1;
\par      dich \tab := m+n+2;
\par      maxtc \tab := 0;
\par      mintc \tab := 0;
\par      for i:=1 to m do
\par      begin
\par \tab       for j:=1 to n do
\par \tab       begin
\par \tab \tab        read(f,ch);
\par \tab \tab        if ch='1' then
\par                  begin
\par                     c[i+1,j+m+1]:=1;
\par                     inc(tc[j]);
\par                     if maxtc<tc[j] then maxtc := tc[j];
\par                     if mintc>tc[j] then mintc := tc[j];
\par                  end;
\par \tab \tab  end;
\par \tab       readln(f);
\par \tab end;
\par      close(f);
\par      sc:=0;
\par      for i:=1 to n do
\par          if tc[i]>0 then sc:=sc+1;
\par end;
\par 
\par procedure lamvoi(k : byte);
\par var i,j : byte;
\par begin
\par      for i:=2 to m+1  do c[1,i] := 2;
\par      for i:=m+2 to dich-1 do c[i,dich] := k;
\par 
\par end;
\par 
\par function min(a,b : integer): integer;
\par begin
\par      if a<b then min:=a else min:=b;
\par end;
\par 
\par procedure khoitao;
\par begin
\par      fillchar(d,sizeof(d),false);
\par      with nh[xp] do
\par           begin
\par                truoc := +1;
\par                delta :=  255;
\par           end;
\par      d[xp] \tab := true;
\par      top \tab \tab := 1;
\par      s[top] \tab := xp;
\par end;
\par 
\par var f : ^kmang;
\par 
\par procedure gannhan(j : byte);
\par var i,td,tc : integer;
\par begin
\par     if (j<=m+1) and (j<>1) then
\par         begin
\par              td:=m+2;
\par              tc:=dich;
\par         end
\par     else
\par     begin
\par       td:=2;
\par       tc:=m+1;
\par       if (c[j,dich]>0)and(f^[j,dich]<c[j,dich])
\par                       and(not d[dich]) then 
\par       begin
\par         with nh[dich] do
\par          begin
\par             truoc := +j;
\par             delta := min(nh[j].delta,c[j,dich]-f^[j,dich]);
\par          end;
\par          d[dich] := true;
\par          inc(top);
\par          s[top] := dich;
\par       end
\par     end;
\par     for i:=td to tc do
\par     begin
\par       if (c[j,i]>0) and (f^[j,i]<c[j,i])
\par                  and (not d[i]) then
\par       begin
\par            with nh[i] do
\par            begin
\par                truoc := +j;
\par                delta := min(nh[j].delta,c[j,i]-f^[j,i]);
\par            end;
\par            d[i] := true;
\par            inc(top);
\par            s[top] := i;
\par       end
\par       else
\par       if (c[i,j]>0) and (f^[i,j]>0) and (not d[i]) then
\par       begin
\par          with nh[i] do
\par              begin
\par                  truoc := -j;
\par                  delta := min(nh[j].delta,f^[i,j]);
\par              end;
\par          d[i] := true;
\par          inc(top);
\par          s[top] := i;
\par       end
\par   end;
\par end;
\par 
\par procedure dieuchinh;
\par var i,j : integer;
\par begin
\par      i := dich;
\par      repeat
\par      j := i;
\par      i := nh[j].truoc;
\par      if i>0 then f^[i,j] := f^[i,j]+nh[dich].delta
\par      else
\par      if i<0 then f^[j,-i] := f^[j,-i]-nh[dich].delta;
\par      i := abs(i);
\par      until i= xp;
\par end;
\par 
\par function xaydung: boolean;
\par var i,j : integer; 
\par begin
\par      repeat
\par      khoitao;
\par      while (top>0) do
\par            begin
\par                 j := s[top];
\par                 dec(top);
\par                 gannhan(j);
\par                 if d[dich] then
\par         \tab \tab  begin
\par              \tab \tab dieuchinh ;
\par              \tab \tab break;
\par         \tab \tab  end
\par            end;
\par      until not d[dich];
\par }\pard \s16\fi720\nowidctlpar\adjustright {\fs24 j:=0;
\par }\pard \s16\nowidctlpar\adjustright {\fs24      for i:=m+2 to dich-1 do
\par          if f^[i,dich]>0 then j:=j+f^[i,dich];
\par      xaydung:=j=m*2;
\par end;
\par 
\par procedure hienkq;
\par 
\par var i,j ,t  : integer;
\par     out     : text;
\par     ok      : boolean;
\par begin
\par      assign(out,fo);
\par      rewrite(out);
\par      writeln(out,k);
\par      for i:=2 to m+1 do
\par          begin
\par               ok := false;
\par               for j:=m+2 to dich-1 do
\par                   if f^[i,j]>0 then
\par                      begin
\par                           \{write(out,(j-m-1):4);\}
\par                           write(out,1);
\par                           ok := true;
\par                      end
\par                   else write(out,0);
\par               if not ok then write(out,0:4);
\par               writeln(out);
\par           end;
\par      close(out);
\par end;
\par 
\par begin
\par         clrscr;
\par         new(f);
\par         taof;
\par         docf;
\par         \{k:=maxtc;\}k:=m*2 div sc;
\par         ltime:=time;
\par         repeat
\par               lamvoi(k);
\par               fillchar(f^,sizeof(f^),0);
\par               if xaydung then
\par               begin
\par                    \{lk := k;
\par                    k:= (maxtc+mintc) div 2 ;
\par                    maxtc := k;\}
\par                    hienkq;
\par                    writeln((time-ltime) /18.2:8:2);
\par                    halt;
\par               end
\par               else \{mintc := k\} inc(k);
\par         until \{(k*n<2*m) or (k=mintc)\} k=maxtc;
\par         k := lk;
\par         hienkq;
\par         writeln((time-ltime) /18.2:8:2);
\par end.
\par }\pard \s16\qj\fi360\nowidctlpar\adjustright {\f15\fs24 
\par }\pard \s16\nowidctlpar\adjustright {\b\f15\fs28\ul B\'b5i 3 :}{\f15\fs28    C\'b8ch 2
\par }{\fs24 
\par program luong2p;
\par uses crt;
\par const max\tab = 100;
\par       fi\tab = 'input.txt';
\par       fo\tab = 'output.txt';
\par 
\par type mang\tab = array[1..max,1..max] of byte;
\par      mang2= array[1..max*2] of integer;
\par 
\par var \tab a,l,ll \tab \tab : mang;
\par     \tab b1,b2,c1,c2 \tab : mang2;
\par     \tab tr1,tr2,nh \tab : mang2;
\par     \tab sc \tab \tab \tab : mang2;
\par     \tab m,n \tab \tab \tab : integer;
\par     \tab tren,duoi \tab : integer;
\par     \tab schon,ls \tab \tab : integer;
\par     \tab li,lj \tab \tab : integer;
\par     \tab lmin \tab \tab : integer;
\par     \tab f \tab \tab \tab : text;
\par 
\par procedure read_inp;
\par var i,j \tab : integer;
\par     ch \tab : char;
\par begin
\par      assign(f,fi); reset(f);
\par      readln(f,m,n);
\par      for i:=1 to m do
\par          begin
\par               for j:=1 to n do
\par                   begin
\par                        read(f,ch);
\par                        a[i,j]:=ord(ch)-ord('0');
\par                   end;
\par               readln(f);
\par          end;
\par      close(f);
\par end;
\par 
\par procedure write_out;
\par var i,j : integer;
\par begin
\par      assign(f,fo); rewrite(f);
\par      writeln(f,ls);
\par      for i:=1 to m do
\par          begin
\par               for j:=1 to n do write(f,ll[i,j]);
\par               writeln(f);
\par          end;
\par      close(f);
\par end;
\par 
\par procedure init_data;
\par var i,j : integer;
\par begin
\par      for i:=1 to m do
\par          for j:=1 to n do
\par              if a[i,j]=1 then inc(sc[j]);
\par end;
\par 
\par function min(a,b : integer) : integer;
\par begin
\par      if a<b then min:=a
\par      else min:=b;
\par end;
\par 
\par procedure gan;
\par var i : integer;
\par begin
\par      for i:=1 to m do b1[i]:=2;
\par      for i:=1 to n do b2[i]:=min(schon,sc[i]);
\par end;
\par 
\par function find_path( k : integer) : boolean;
\par var q \tab \tab : mang2;
\par     dau,cuoi \tab : integer;
\par     i,j \tab \tab : integer;
\par begin
\par      fillchar(tr1,sizeof(tr1),0);
\par      fillchar(tr2,sizeof(tr2),0);
\par      fillchar(nh,sizeof(nh),0); tr1[k]:=-1;
\par      dau:=0; cuoi:=1;q[cuoi]:=k; nh[1]:=maxint;
\par      find_path:=true;
\par      while dau<cuoi do
\par      begin
\par          inc(dau); i:=q[dau];
\par          if i<=m then
\par          begin
\par               for j:=1 to n do
\par               if (a[i,j]>l[i,j]) and (tr2[j]=0) then
\par               begin
\par                   inc(cuoi); q[cuoi]:=j+m;
\par                   nh[cuoi]:=min(nh[dau],a[i,j]-l[i,j]);
\par                   tr2[j]:=i;
\par                   if c2[j]<b2[j] then
\par                   begin
\par                        li:=i; lj:=j;lmin:=nh[cuoi];
\par                        exit;
\par                   end;
\par               end;
\par          end
\par          else
\par          begin
\par               j:=i-m;
\par               for i:=1 to m do
\par               if (l[i,j]>0) and (tr1[i]=0) then
\par               begin
\par                   inc(cuoi); q[cuoi]:=i;
\par                   tr1[i]:=j;
\par                   nh[cuoi]:=min(nh[dau],l[i,j]);
\par               end;
\par          end;
\par      end;
\par      find_path:=false;
\par end;
\par 
\par procedure change_path;
\par var i,j,k : integer;
\par begin
\par      i:=li; j:=lj;
\par      inc(c2[j],lmin);
\par      inc(l[i,j],lmin);
\par      if tr1[i]>0 then
\par         repeat
\par               j:=tr1[i];
\par               k:=tr2[j];
\par               dec(l[i,j],lmin);
\par               inc(l[k,j],lmin);
\par               i:=k;
\par         until tr1[i]<=0;
\par      inc(c1[i]);
\par end;
\par 
\par function ok : boolean;
\par var i : integer;
\par begin
\par      ok:=false;
\par      fillchar(c1,sizeof(c1),0);
\par      fillchar(c2,sizeof(c2),0);
\par      fillchar(l,sizeof(l),0);
\par      for i:=1 to m do
\par          begin
\par               while b1[i]>c1[i] do
\par                     begin
\par                          if find_path(i) then
\par                             change_path
\par                          else break;
\par                     end;
\par               if b1[i]>c1[i] then exit;
\par          end;
\par      ok:=true;
\par end;
\par 
\par procedure solution;
\par var giua : integer;
\par begin
\par      init_data;
\par      tren:=m; duoi:=0;
\par      repeat
\par            schon:=(tren+duoi) div 2;
\par            gan;
\par            if ok then
\par               begin
\par                    tren:=schon-1;
\par                    ls:=schon;
\par                    ll:=l;
\par               end
\par            else duoi:=schon+1;
\par      until tren<duoi;
\par end;
\par 
\par BEGIN
\par      read_inp;
\par      solution;
\par      write_out;
\par END.
\par }\pard\plain \s15\qc\nowidctlpar\adjustright \f18\fs28 {Lu\'e5ng tr\'aan \'ae\'e5 th\'de hai ph\'dda
\par }\pard\plain \qc\nowidctlpar\adjustright \f15 {\fs20 
\par }\pard\plain \s15\qc\nowidctlpar\adjustright \f18\fs28 {\b S\'edm nh\'cat}{
\par }\pard\plain \qc\nowidctlpar\adjustright \f15 {\i (B\'b5i 4-\'a7\'d2 thi ch\'ddnh th\'f8c k\'fa thi h\'e4c sinh gi\'e1i qu\'e8c gia l\'edp 12 THPT 1999-2000)
\par 
\par }\pard\plain \s28\fi720\sa120\nowidctlpar\adjustright \f15 {T\'b9i th\'eai \'aei\'d3m 0, m\'e9t C\'abng ty d\'dech v\'f4 ph\'b8t tri\'d3n ph\'c7n m\'d2m nh\'cbn th\'f9c hi\'d6n N y\'aau c\'c7u c\'f1a kh\'b8ch h\'b5ng (\'ae\'ad\'eec \'ae\'b8nh s\'e8 t\'f5
 1 \'ae\'d5n N) . M\'e7i y\'aau c\'c7u c\'f1a kh\'b8ch h\'b5ng thu\'e9c m\'e9t trong K l\'dcnh v\'f9c chuy\'aan m\'abn ( \'ae\'ad\'eec \'ae\'b8nh s\'e8 t\'f5 1 \'ae\'d5n K) v\'b5 c\'e3 m\'e9t trong 3 m\'f8c \'ae\'e9 kh\'e3 ( \'ae\'ad\'eec \'ae\'b8nh s\'e8
 t\'f5 1 \'ae\'d5n 3) C\'abng ty c\'e3 M chuy\'aan vi\'aan (\'ae\'ad\'eec \'ae\'b8nh s\'e8 t\'f5 1 \'ae\'d5n M) \'ae\'d3 th\'f9c hi\'d6n c\'b8c y\'aau c\'c7u n\'b5y. M\'e7i chuy\'aan vi\'aan ch\'d8 c\'e3 th\'d3 th\'f9c hi\'d6n c\'b8c c\'abng vi\'d6
c trong m\'e9t s\'e8 l\'dcnh v\'f9c chuy\'aan m\'abn thu\'e9c K l\'dcnh v\'f9c n\'aau tr\'aan. C\'abng ty bi\'d5t th\'eai gian m\'b5 m\'e7i nh\'a9n vi\'aan c\'c7n \'ae\'d3 th\'f9c hi\'d6n c\'b8c y\'aau c\'c7u \'eb c\'b8c m\'f8c \'ae\'e9 kh\'e3 kh\'b8
c nhau trong m\'e7i l\'dcnh v\'f9c chuy\'aan m\'abn.
\par }\pard\plain \qj\nowidctlpar\adjustright \f15 {\b Y\'aau c\'c7u :}{ T\'d7m c\'b8ch b\'e8 tr\'dd c\'b8c chuy\'aan vi\'aan th\'f9c hi\'d6n c\'b8c y\'aau c\'c7u c\'f1a kh\'b8ch h\'b5ng sao cho m\'e7i y\'aau c\'c7u c\'f1a kh\'b8ch h\'b5ng \'ae\'ad\'ee
c giao cho \'ae\'f3ng m\'e9t nh\'a9n vi\'aan c\'e3 kh\'b6 n\'a8ng th\'f9c hi\'d6n v\'b5 th\'eai \'aei\'d3m ho\'b5n th\'b5nh t\'cat c\'b6 c\'b8c y\'aau c\'c7u c\'f1a kh\'b8ch h\'b5ng l\'b5 s\'edm nh\'cat.
