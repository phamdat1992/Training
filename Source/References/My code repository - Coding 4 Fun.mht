#include &lt;algorithm&gt;=0A=
#include &lt;iostream&gt;=0A=
#include &lt;stdio.h&gt;=0A=
#include &lt;queue&gt;=0A=
#define MAX 1000=0A=
#define NIL -1=0A=
using namespace std;=0A=
=0A=
int Graph[MAX][MAX],flow[MAX][MAX],ResidualGraph[MAX][MAX];=0A=
int Totaledges,TotalVertices;=0A=
int color[MAX],parent[MAX],Distance[MAX];=0A=
vector&lt;int&gt;path;=0A=
void print_path(int s,int v)=0A=
{=0A=
	if(v=3D=3Ds)=0A=
		path.push_back(s);=0A=
	else if(Distance[v]=3D=3DNIL)=0A=
	{}=0A=
	else=0A=
	{=0A=
		print_path(s,parent[v]);=0A=
		path.push_back(v);=0A=
	}=0A=
}=0A=
bool bfs(int source,int sink)=0A=
{=0A=
	int i,j,u,v;=0A=
	const int WHITE=3D0,GRAY=3D1,BLACK=3D2;=0A=
	queue&lt;int&gt;Queue;=0A=
	//Initialise all the vertices to white=0A=
	for(i=3D0;i&lt; TotalVertices+1;++i)=0A=
	{=0A=
		color[i]=3DWHITE;										//Initially all the vertices are =
undiscovered=0A=
		parent[i]=3DNIL;=0A=
		Distance[i]=3DMAX;=0A=
	}=0A=
=0A=
	//Enque the Start Vertex(Source) and Paint it Gray=0A=
	Queue.push(source);=0A=
	color[source]=3DGRAY;=0A=
	Distance[source]=3D0;=0A=
=0A=
	//Until queue is empty=0A=
	while(!Queue.empty())		=0A=
	{=0A=
		u=3DQueue.front();									//Dequeue the gray painted Vertex at the =
head of the queue =0A=
		Queue.pop();=0A=
		for(i=3D1;i&lt;=3DTotalVertices;++i)						//For all adjacent Vertices =0A=
		{=0A=
			for(j=3D1;j&lt;=3DTotalVertices;++j)=0A=
			{=0A=
				v=3Dj;							=0A=
				if(ResidualGraph[u][v]!=3D0 &amp;&amp; color[v]=3D=3DWHITE)//Is the =
Adjacent Vertex Undiscovered		=0A=
				{=0A=
					Queue.push(v);	=0A=
					color[v]=3DGRAY;							 //The vertices in queue are always Gray =
painted=0A=
					Distance[v]=3DDistance[u]+1;=0A=
					parent[v]=3Du;=0A=
					if(v=3D=3Dsink)								//When the target is found ,just stop Dfs=0A=
						return true;						//Yup!!!! Sink still Reachable frm source on in =
Residual Network=0A=
				}=0A=
			}=0A=
			//Paint the Dequeued Vertex BLACK=0A=
			color[u]=3DBLACK;=0A=
		}=0A=
	}=0A=
	return false;=0A=
}=0A=
//FORD-FULKERSON=0A=
int maxflow(int source,int sink)=0A=
{=0A=
	int i,Rescap,x,y,max_flow=3D0;=0A=
	/*1.=0A=
	for each edge (u,v)E Graph =0A=
	flow[u,v]:=3D0=0A=
	Already Done =0A=
	*/=0A=
	/*2. while there exists a path p from s to t in the residual network =
Gf*/=0A=
	while((bfs(source,sink)))								//True -&gt; There exists an =
augmenting Path=0A=
	{=0A=
		Rescap=3D1&lt;&lt;30;										=0A=
		print_path(source,sink);							//Just Trace the Augmented Path=0A=
		for(i=3D0;i&lt;path.size()-1;++i)=0A=
		{=0A=
			x=3Dpath[i],y=3Dpath[i+1];			=0A=
			if(ResidualGraph[x][y]&lt;Rescap)					//Res Cap(p) :=3Dmin capacity =
in Residual network augmented path p.=0A=
				Rescap=3DResidualGraph[x][y];=0A=
		}=0A=
		/*for each edge (x,y) in augmented path=0A=
		if x,y E Graph(originalNetwork).edge=0A=
		flow[x][y]+=3DResidual Capacity.=0A=
		else =0A=
		flow[y][x]-=3DResidual Capacity.=0A=
		*/=0A=
		for(i=3D0;i&lt;path.size()-1;++i)=0A=
		{=0A=
			x=3Dpath[i],y=3Dpath[i+1];=0A=
			if(Graph[x][y]&gt;0)=0A=
				flow[x][y]+=3DRescap;=0A=
			else=0A=
				flow[y][x]-=3DRescap;=0A=
=0A=
			/*This one is important :=0A=
			We construct Residual Network by drawing the Back edge with flow =
value.=0A=
			ie --(f,C)-&gt;  .=0A=
			Then Residual Network is &lt;------------f------------------=0A=
			----OriginalCapaciy minus f ---&gt;=0A=
			*/=0A=
			ResidualGraph[y][x]=3Dflow[x][y];=0A=
			ResidualGraph[x][y]=3DGraph[x][y]-flow[x][y];		//Means I can send =
ResidualGraph[x][y] more through x,y pipe.=0A=
		}=0A=
		path.clear();=0A=
	}=0A=
	//Calculate Max Flow=0A=
	for(i=3D1;i&lt;=3DTotalVertices;++i)=0A=
		max_flow=3Dmax_flow+flow[i][sink];=0A=
	return max_flow;=0A=
}=0A=
//Clear array for another Testcases=0A=
void reset()=0A=
{=0A=
	for(int i=3D0;i&lt;TotalVertices+1;i++)=0A=
		for(int j=3D0;j&lt;TotalVertices+1;j++)=0A=
			flow[i][j]=3DGraph[i][j]=3DResidualGraph[i][j]=3D0;=0A=
}=0A=
int main()=0A=
{=0A=
	int i,u,v,flowcap,source,sink,Kases=3D1;=0A=
	scanf("%d",&amp;TotalVertices);=0A=
	for(;TotalVertices!=3D0;)=0A=
	{=0A=
		reset();=0A=
		scanf("%d%d%d",&amp;source,&amp;sink,&amp;Totaledges);=0A=
		for(i=3D1;i&lt;=3DTotaledges;++i)=0A=
		{=0A=
			scanf("%d %d %d",&amp;u,&amp;v,&amp;flowcap);=0A=
			Graph[u][v]=3DGraph[u][v]+(flowcap);=0A=
			Graph[v][u]=3DGraph[v][u]+(flowcap);			//Uncomment this statement if =
the Graph is Bidirectional=0A=
=0A=
			//Initially the Residual Graph=0A=
			ResidualGraph[u][v]=3DGraph[u][v];=0A=
			ResidualGraph[v][u]=3DGraph[u][v];			//Uncomment this statement if =
the Graph is Bidirectional=0A=
=0A=
		}=0A=
		printf("Network %d\nThe bandwidth is =
%d.\n\n",Kases,maxflow(source,sink));=0A=
		Kases++;=0A=
		scanf("%d",&amp;TotalVertices);=0A=
	}=0A=
	return 0;=0A=
}=0A=
=0A=
</CODE>=0A=
