<P><B style=3D'color: rgb(34, 34, 128); font-family: "Arial =
Black";'><FONT size=3D"3">Problem=20
1:Simple problem on building a segment tree and maximum=20
query.<BR></FONT></B><FONT face=3D"Arial Black"><FONT =
color=3D"#222280"><B><FONT=20
size=3D"1">Wiki Implementation :<A class=3D"fw_link_website =
fw_link_newWindow" =
href=3D"http://wcipeg.com/wiki/Segment_tree#Maximum.2Fminimum_prefix.2Fsu=
ffix_sum">http://wcipeg.com/wiki/Segment_tree#Maximum.2Fminimum_prefix.2F=
suffix_sum</A></FONT><BR></B></FONT></FONT>
<DIV style=3D"width: auto; overflow: auto; background-color: rgb(238, =
238, 238);"><PRE style=3D"color: rgb(0, 0, 0); font-size: 15px; =
background-color: rgb(238, 238, 238);"><CODE>=0A=
/*Tested at =0A=
http://www.spoj.pl/problems/THRBL/=0A=
Wiki Implementation =
:http://wcipeg.com/wiki/Segment_tree#Maximum.2Fminimum_prefix.2Fsuffix_su=
m=0A=
*/=0A=
#include &lt;iostream&gt;=0A=
#include &lt;algorithm&gt;=0A=
#include &lt;vector&gt;=0A=
#define MAX 1000000=0A=
using namespace std;=0A=
int A[MAX],Tree[4*MAX];=0A=
void build(int Nodenumber,int begin,int end)=0A=
{=0A=
	if(begin&gt;end)=0A=
		return;=0A=
	if(begin=3D=3Dend)=0A=
	{=0A=
		Tree[Nodenumber]=3DA[begin];=0A=
		return;=0A=
	}=0A=
	build(Nodenumber*2,begin,(begin+end)/2);=0A=
	build(Nodenumber*2+1,((begin+end)/2)+1,end);=0A=
	Tree[Nodenumber]=3Dmax(Tree[(Nodenumber*2)],Tree[Nodenumber*2+1]);=0A=
}=0A=
int query(int Nodenumber,int t_begin,int t_end,int a_begin,int a_end)=0A=
{=0A=
	if (t_begin&gt;=3Da_begin &amp;&amp; t_end&lt;=3Da_end)=0A=
		return Tree[Nodenumber];=0A=
	else=0A=
	{=0A=
		int mid=3D((t_begin+t_end)/2);=0A=
		int res =3D -1;=0A=
=0A=
		if (mid&gt;=3Da_begin &amp;&amp; t_begin&lt;=3Da_end)=0A=
			res =3D max(res,query(2*Nodenumber,t_begin,mid,a_begin,a_end));=0A=
=0A=
		if (t_end&gt;=3Da_begin &amp;&amp; mid+1&lt;=3Da_end)=0A=
			res =3D max(res,query(2*Nodenumber+1,mid+1,t_end,a_begin,a_end));=0A=
=0A=
		return res;=0A=
	}=0A=
}=0A=
int main()=0A=
{=0A=
	int N,TotalQueries,finalans=3D0,QA,QB;=0A=
	scanf("%d%d",&amp;N,&amp;TotalQueries);=0A=
	for(int i=3D0;i&lt;N;++i)=0A=
		scanf("%d",&amp;A[i]);=0A=
=0A=
	build(1,0,N-1);=0A=
=0A=
	/*=0A=
	To make queries,ie MAx element between Index QA and QB is=0A=
	:=3Dquery(1,0,N-1,QA,QB);=0A=
	*/=0A=
	/*FOR SOLVING SPOJ PROBLEM STATEMENT*/=0A=
	for(int i=3D1;i&lt;=3DTotalQueries;++i)=0A=
	{=0A=
		scanf("%d%d",&amp;QA,&amp;QB);=0A=
		QA-=3D1;=0A=
		QB-=3D1;=0A=
		int tmp=3Dquery(1,0,N-1,QA,QB-1);=0A=
		if(tmp&lt;=3DA[QA])=0A=
			finalans+=3D1;=0A=
	}=0A=
	printf("%d\n",finalans);=0A=
	//system("pause");=0A=
	return 0;=0A=
}=0A=
</CODE>=0A=
