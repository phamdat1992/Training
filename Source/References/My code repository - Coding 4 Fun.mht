#include &lt;string&gt;=0A=
#include &lt;cstring&gt;=0A=
#include &lt;cmath&gt;=0A=
#define MAX 100001=0A=
=0A=
=0A=
using namespace std;=0A=
char str[MAX]; //input=0A=
int Rank[MAX], suffixArray[MAX]; //output=0A=
int cnt[MAX], Next[MAX]; //internal=0A=
bool bh[MAX], b2h[MAX];=0A=
int Height[MAX];=0A=
int Length_of_str;=0A=
/*The format for M table where preprocessing value are stored is=0A=
M[MAX_STRING_SIZE][logbase2(MAX_STRING_SIZE)].=0A=
Also it it observed that Value of logbase2(10^7)=3D 23.253496664.=0A=
Thus always fix logbase2 value to 25.=0A=
*/=0A=
=0A=
int M[MAX][25];=0A=
=0A=
bool smaller_first_char(int a, int b)=0A=
{=0A=
	return str[a] &lt; str[b];=0A=
}=0A=
void print(int index)=0A=
{=0A=
	for(int i=3Dindex;i&lt;strlen(str);++i)=0A=
	{=0A=
		cout&lt;&lt;str[i];=0A=
	}=0A=
