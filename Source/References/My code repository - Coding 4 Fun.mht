	for(int i=3D1;i&lt;Len;++i)=0A=
	{=0A=
		if((Height[i]&gt;res))=0A=
		{=0A=
			if((suffixArray[i-1]&lt;actuallen &amp;&amp; =
suffixArray[i]&gt;actuallen)||(suffixArray[i]&lt;actuallen &amp;&amp; =
suffixArray[i-1]&gt;actuallen))=0A=
			{=0A=
				res=3DHeight[i];=0A=
				startptr=3DsuffixArray[i];=0A=
			}=0A=
