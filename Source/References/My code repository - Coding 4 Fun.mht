	{=0A=
		return Height[RMQ(i+1,j)];=0A=
		//LCP(suffix i,suffix j)=3DLCPadj[RMQ(i + 1; j)] =0A=
		//LCPadj=3DLCP of adjacent suffix =3DHeight.=0A=
	}=0A=
}=0A=
int main()=0A=
{=0A=
=0A=
	int Len,actuallen;=0A=
	char tc[10],Lenbuffer[10];=0A=
=0A=
	scanf("%d",&amp;Len);=0A=
	scanf("%s",str);=0A=
=0A=
	Len=3Dstrlen(str);=0A=
	actuallen=3DLen;=0A=
=0A=
	strcpy(original,str);=0A=
	str[Len]=3D'#';			//Put Marker=0A=
	str[Len+1]=3D0;=0A=
=0A=
	reverse(original,original+Len);=0A=
=0A=
	//Concatenate to form "string#gnirts"=0A=
	strcat(str,original);=0A=
=0A=
	//Update the Length of new string . Updated =
Length=3DLen(original)+Len(Reverse)+LenMarher (Marker '#' Len=3D1)=0A=
	Len=3D(actuallen*2)+1;=0A=
=0A=
	suffixSort(Len);=0A=
=0A=
	/*Calculate LCP of Adjacents /consecutive Suffix Pairs */=0A=
	getHeight(Len);=0A=
=0A=
	/*Calculate LCP of Any two suffixes i,j using RMQ concept */=0A=
	//preprocesses(Len);=0A=
=0A=
	/* In general we find the character just after our current position=0A=
	in the original string, and the character just before it in the =
reversed string, =0A=
	and find the longest common prefix using the lcp array and RMQ=0A=
	*/=0A=
	int res=3D0;=0A=
