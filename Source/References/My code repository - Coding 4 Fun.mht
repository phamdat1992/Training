<P><B><FONT size=3D"4"><FONT color=3D"#222280">Implementation 3: =
&nbsp;</FONT><A=20
class=3D"fw_link_website fw_link_newWindow" style=3D"color: rgb(34, 34, =
128);" href=3D"http://www.spoj.com/problems/SUBLEX/">Lexicographical=20
Substring Search</A><BR><FONT color=3D"#222280">Refer following =
</FONT><A class=3D"fw_link_website fw_link_newWindow"=20
href=3D"http://stackoverflow.com/a/9390461/1348550"><FONT=20
color=3D"#292422">Discussions</FONT></A></FONT></B></P>
<P>&nbsp;
<DIV style=3D"width: auto; overflow: auto; background-color: rgb(238, =
238, 238);"><PRE style=3D"color: rgb(0, 0, 0); font-size: 15px; =
background-color: rgb(238, 238, 238);"><CODE>=0A=
/*=0A=
Tested at:=0A=
http://www.spoj.pl/problems/SUBLEX/=0A=
If all distinct substrings of string S were sorted lexicographically, =
which one will be the K-th smallest?=0A=
=0A=
Thanks to this discussions =0A=
http://stackoverflow.com/a/9390461/1348550=0A=
*/=0A=
=0A=
#include &lt;stdio.h&gt;=0A=
#include &lt;algorithm&gt;=0A=
#include &lt;iostream&gt;=0A=
#include &lt;string&gt;=0A=
#include &lt;cstring&gt;=0A=
#define MAX 90099=0A=
using namespace std;=0A=
char str[MAX]; //input=0A=
int Rank[MAX], suffixArray[MAX]; //output=0A=
int cnt[MAX], Next[MAX]; //internal=0A=
bool bh[MAX], b2h[MAX];=0A=
int Height[MAX];=0A=
int AccumulatedCount[MAX];=0A=
=0A=
// Compares two suffixes according to their first characters=0A=
bool smaller_first_char(int a, int b)=0A=
{=0A=
	return str[a] &lt; str[b];=0A=
}=0A=
void print(int index)=0A=
{=0A=
	for(int i=3Dindex;i&lt;strlen(str);++i)=0A=
	{=0A=
		cout&lt;&lt;str[i];=0A=
	}=0A=
	cout&lt;&lt;endl;=0A=
}=0A=
=0A=
void suffixSort(int n){=0A=
	//sort suffixes according to their first characters=0A=
	for (int i=3D0; i&lt;n; ++i){=0A=
		suffixArray[i] =3D i;=0A=
	}=0A=
	sort(suffixArray, suffixArray + n, smaller_first_char);=0A=
	//{suffixArray contains the list of suffixes sorted by their first =
character}=0A=
=0A=
	for (int i=3D0; i&lt;n; ++i){=0A=
		bh[i] =3D i =3D=3D 0 || str[suffixArray[i]] !=3D str[suffixArray[i-1]];=0A=
		b2h[i] =3D false;=0A=
	}=0A=
=0A=
	for (int h =3D 1; h &lt; n; h &lt;&lt;=3D 1){=0A=
		//{bh[i] =3D=3D false if the first h characters of suffixArray[i-1] =
=3D=3D the first h characters of suffixArray[i]}=0A=
		int buckets =3D 0;=0A=
		for (int i=3D0, j; i &lt; n; i =3D j){=0A=
			j =3D i + 1;=0A=
			while (j &lt; n &amp;&amp; !bh[j]) j++;=0A=
			Next[i] =3D j;=0A=
			buckets++;=0A=
		}=0A=
		if (buckets =3D=3D n) break; // We are done! Lucky bastards!=0A=
		//{suffixes are separted in buckets containing strings starting with =
the same h characters}=0A=
=0A=
		for (int i =3D 0; i &lt; n; i =3D Next[i]){=0A=
			cnt[i] =3D 0;=0A=
			for (int j =3D i; j &lt; Next[i]; ++j){=0A=
				Rank[suffixArray[j]] =3D i;=0A=
			}=0A=
		}=0A=
=0A=
		cnt[Rank[n - h]]++;=0A=
		b2h[Rank[n - h]] =3D true;=0A=
		for (int i =3D 0; i &lt; n; i =3D Next[i]){=0A=
			for (int j =3D i; j &lt; Next[i]; ++j){=0A=
				int s =3D suffixArray[j] - h;=0A=
				if (s &gt;=3D 0){=0A=
					int head =3D Rank[s];=0A=
					Rank[s] =3D head + cnt[head]++;=0A=
					b2h[Rank[s]] =3D true;=0A=
				}=0A=
			}=0A=
			for (int j =3D i; j &lt; Next[i]; ++j){=0A=
				int s =3D suffixArray[j] - h;=0A=
				if (s &gt;=3D 0 &amp;&amp; b2h[Rank[s]]){=0A=
					for (int k =3D Rank[s]+1; !bh[k] &amp;&amp; b2h[k]; k++) b2h[k] =3D =
false;=0A=
				}=0A=
			}=0A=
		}=0A=
		for (int i=3D0; i&lt;n; ++i){=0A=
			suffixArray[Rank[i]] =3D i;=0A=
			bh[i] |=3D b2h[i];=0A=
		}=0A=
	}=0A=
	for (int i=3D0; i&lt;n; ++i)=0A=
	{=0A=
		Rank[suffixArray[i]] =3D i;=0A=
	}=0A=
}=0A=
// End of suffix array algorithm=0A=
=0A=
=0A=
=0A=
void getHeight(int n)=0A=
{=0A=
	for (int i=3D0; i&lt;n; ++i) Rank[suffixArray[i]] =3D i;=0A=
	Height[0] =3D 0;=0A=
	for (int i=3D0, h=3D0; i&lt;n; ++i)=0A=
	{=0A=
		if (Rank[i] &gt; 0)=0A=
		{=0A=
			int j =3D suffixArray[Rank[i]-1];=0A=
			while (i + h &lt; n &amp;&amp; j + h &lt; n &amp;&amp; str[i+h] =
=3D=3D str[j+h])=0A=
			{=0A=
				h++;=0A=
			}=0A=
			Height[Rank[i]] =3D h;=0A=
			if (h &gt; 0) h--;=0A=
		}=0A=
	}=0A=
}=0A=
// End of longest common prefixes algorithm=0A=
int main()=0A=
{=0A=
	int kth,T, diff,suffixsize;=0A=
	gets(str);=0A=
	int Len=3Dstrlen(str);=0A=
	scanf("%d",&amp;T);=0A=
	suffixSort(Len);=0A=
	getHeight(Len);=0A=
=0A=
	/*******************************FIND THE KTH LEXOGRAPHIC SUBSTR STARTS =
HERE ***********/=0A=
	int sum=3D0;=0A=
	for(int i=3D0;i&lt;Len;++i)=0A=
	{=0A=
		suffixsize=3D(Len-suffixArray[i]);=0A=
		diff=3Dsuffixsize-Height[i];					/*Note that the diff is the total =
distinct substrings that a suffix i will  give*/=0A=
		sum=3Dsum+diff;=0A=
		AccumulatedCount[i]=3Dsum;=0A=
	}=0A=
=0A=
	/*So Till Here I have cached accumulated count also.*/=0A=
	while(T--)=0A=
	{=0A=
		scanf("%d",&amp;kth);						//Kth Lexographic Substring=0A=
=0A=
		//Step 1:  find the first entry that has an accumulated count greater =
than or equal to K=0A=
		int FirstEntry=3D0;=0A=
		for(int i=3D0;i&lt;Len;++i)=0A=
		{=0A=
			if(AccumulatedCount[i]&gt;=3Dkth)=0A=
			{=0A=
				FirstEntry=3Di;=0A=
				break;=0A=
			}=0A=
		}=0A=
=0A=
		//Then remove the prefix it shares with the previous entry.This is =
equivalent to LCP[i].=0A=
		int sharedprefixLength=3DHeight[FirstEntry];=0A=
=0A=
		/*So the Answer is the common prefix + Total (Kth-accumulated =
[entry-1]) character(after common prefiexes ) from FirstEntry suffix*/=0A=
=0A=
		//Lets print the COMMON prefix First=0A=
		int Idx,j;=0A=
		=
for(Idx=3DsuffixArray[FirstEntry],j=3D0;j&lt;sharedprefixLength;++j,++Idx=
)=0A=
			putc(str[Idx],stdout);=0A=
=0A=
		// Then print the Total (Kth-accumulated [entry-1]) characters from =
the position after where the common prefix Ends in the Suffix [Entry]=0A=
=0A=
		//Note Idx is at the position just after the common prefix string in =
the suffix[First Entry].=0A=
		//so just print (Kth-accumulated [entry-1]) characters from this =
position.=0A=
=0A=
		for(int j=3D0;j&lt;kth-AccumulatedCount[FirstEntry-1];++j,++Idx)=0A=
			putc(str[Idx],stdout);=0A=
=0A=
		putc('\n',stdout);=0A=
	}=0A=
	return 0;=0A=
}=0A=
=0A=
</CODE>=0A=
</PRE></DIV>
