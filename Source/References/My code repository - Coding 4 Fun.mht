<DIV style=3D"width: auto; overflow: auto; background-color: rgb(238, =
238, 238);"><PRE style=3D"color: rgb(0, 0, 0); font-size: 15px; =
background-color: rgb(238, 238, 238);"><CODE>=0A=
#include &lt;stdio.h&gt;=0A=
#include &lt;stdlib.h&gt;=0A=
#define  Mod 1000000007=0A=
#define  K 2=0A=
typedef long long int Int_64;=0A=
/************************************************************************=
*******************************************************/=0A=
//K is Dim=0A=
//Fn[i]=3Dcoeff_1*Fn(i-alpha)+coeff_2*Fn(i-Beta)+...+coeff_KFn(i-Zeta)=0A=
/************************************************************************=
******************************************************/=0A=
=0A=
/*In Matrix Exponentiation All Arrays are 1 Based.=0A=
Function That Multiply the Two Matrices Matrix1 * Matrix 2=0A=
*/=0A=
=0A=
//Variables=0A=
Int_64   **productMatrix;=0A=
Int_64   F1[K+1];=0A=
Int_64   **X;=0A=
Int_64   **T;=0A=
=0A=
// Returns the Simple Matrix Multiplication of Two Matrices M1 and M2.=0A=
Int_64** Multiply(Int_64 **M1,Int_64 **M2)=0A=
{=0A=
	int i,j,k;=0A=
	productMatrix=3D(Int_64**)malloc((K+2) * sizeof(Int_64*));=0A=
	for(i=3D0;i&lt;=3DK;++i)=0A=
		productMatrix[i]=3D(Int_64*)malloc((K+1) *sizeof(Int_64));=0A=
=0A=
	for(i=3D1;i&lt;=3DK;++i)=0A=
	{=0A=
		for(j=3D1;j&lt;=3DK;++j)=0A=
		{=0A=
			productMatrix[i][j]=3D0;=0A=
			for(k=3D1;k&lt;=3DK;++k)=0A=
			{=0A=
				productMatrix[i][j]=3D( productMatrix[i][j]+(M1[i][k]*M2[k][j]) ) % =
Mod;=0A=
			}=0A=
		}=0A=
	}=0A=
	return productMatrix;=0A=
}=0A=
/*=0A=
To solve    :(Matrix)^P ;=0A=
The Following is Logn solution.=0A=
=0A=
|Return  Matrix                      -&gt;if P=3D=3D1;=0A=
(Matrix^P)=3D|Return  (Matrix)*(Matrix^(P-1))     -&gt;if P is Odd=0A=
|Return  (Matrix^P/2)*(Matrix^P/2)   -&gt;if P is Even=0A=
*/=0A=
Int_64** Power(Int_64 **A,Int_64 p)=0A=
{=0A=
	if (p =3D=3D 1)=0A=
		return A;=0A=
	if (p % 2)=0A=
		return  Multiply(A, Power(A, p-1));=0A=
	X =3D Power(A, p/2);=0A=
	return  Multiply(X, X);=0A=
}=0A=
Int_64 solve(Int_64 N)=0A=
{=0A=
	int i;=0A=
	Int_64 result=3D0;=0A=
	/* =0A=
	Example consider the Recurrence Relation to be =0A=
	Fn(N)=3D(2*Fn(N-1)+2*Fn(N-2))=0A=
	Fn(1)=3D1,Fn(K=3D2)=3D3;=0A=
=0A=
	Step 1:=0A=
	As Every DP starts with a base case ,so Similar is the Case Here=0A=
	Base case is a column Vector F1=0A=
	Eg .When FN[i]=3DFN[i-1]+4*FN[i-2]+6*FN[i-3]+8*FN[i-4]=0A=
=0A=
	|FN(0)=3DNULL|=0A=
	|FN(1)     |					  =0A=
	F1:|FN(2)     |=0A=
	|FN(3)     |=0A=
	|FN(K=3D4)   |(K*1 Dimension)=0A=
	*/=0A=
	F1[0]=3D1;=0A=
	F1[1]=3D1;=0A=
	F1[K]=3D3;=0A=
=0A=
	/*Step 2: Fill the T Matrix=0A=
	Eg:FN[i]=3DFN[i-1]+4*FN[i-2]+6*FN[i-3]+8*FN[i-4];=0A=
=0A=
	|N N N N N |=0A=
	|N 0 1 0 0 |=0A=
	T(K*K)=3D |N 0 0 1 0 |=0A=
	|N 0 0 0 1 |=0A=
	|N 8 6 4 1 |        =0A=
	*/=0A=
	T=3D(Int_64**)malloc((K+2) *sizeof(Int_64*));=0A=
	for(i=3D0;i&lt;=3DK;++i)=0A=
		T[i]=3D(Int_64*)malloc((K+1) *sizeof(Int_64));=0A=
=0A=
=0A=
	T[1][1]=3D0,T[1][K]=3D1;=0A=
	T[2][1]=3D2,T[2][K]=3D2;=0A=
=0A=
	/*Step 3:=0A=
	Apply :: T=3D(T^N-1)=0A=
	*/=0A=
	if(N=3D=3D1)=0A=
		return F1[1];=0A=
=0A=
	T=3DPower(T,N-1);=0A=
=0A=
	/*Step 4:=0A=
	So the Overall Answer is::=0A=
	First row of (T*F1).=0A=
	*/=0A=
	result=3D0;=0A=
	for(i=3D1;i&lt;=3DK;++i)=0A=
	{=0A=
		result=3D(result+T[1][i]*F1[i])%Mod;=0A=
	}=0A=
	return result;=0A=
=0A=
}=0A=
int main()=0A=
{=0A=
	int TestCases;=0A=
	Int_64 N;=0A=
	scanf("%d",&amp;TestCases);=0A=
	while(TestCases--)=0A=
	{=0A=
		scanf("%lld",&amp;N);=0A=
		printf("%lld\n",solve(N));=0A=
	}=0A=
	return 0;=0A=
}=0A=
=0A=
</CODE>=0A=
</PRE></DIV>
